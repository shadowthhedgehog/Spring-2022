function y = myFIRfilter(b,x)
%MYFIRFILTER Summary of this function goes here
%   Detailed explanation goes here

persistent z L;

if isempty(z)
   L = length(b);
   z = zeros(size(b));
end

%shift
for n = L:-1:2
   z(n) = z(n-1);
end


%insert
z(1) = x;

%multiply and add
y = 0;
for n = 1:L
   temp = L - (n-1);
   y = y + b(n) * z(temp);
   %y = y + b(n) * z(n);
end
end

